{
  "name": "retrieve-changed-files",
  "description": "GitHub action that retrieves all changed files in a pull request or push.",
  "version": "3.0.0",
  "private": true,
  "author": "MASES s.r.l. (original author Jitterbit, Inc.)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masesgroup/retrieve-changed-files.git"
  },
  "homepage": "https://github.com/masesgroup/retrieve-changed-files#readme",
  "bugs": {
    "url": "https://github.com/masesgroup/retrieve-changed-files/issues"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "TypeScript",
    "JavaScript",
    "Get",
    "Changed",
    "Modified",
    "Diff",
    "Files"
  ],
  "main": "lib/main.js",
  "engines": {
    "node": ">=20.*.*"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/**/* lib/**/*",
    "format": "prettier --write **/*.ts",
	"format-check": "prettier --check \"{,!(node_modules)/**/}*.ts\"",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "yarn clean && yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1"
  },
  "devDependencies": {
    "@octokit/rest": "^22.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.52.0",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^30.0.2",
    "jest-circus": "^30.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.6.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.7.3"
  }
}
